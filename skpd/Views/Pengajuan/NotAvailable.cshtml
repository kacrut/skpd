@model skpd.Models.vwRequest
@{
    ViewBag.Title = "Not Available";
}
<h2>Gagal Pengajuan</h2>
<p>
    Pengajuan tidak boleh lebih dari satu dalam waktu yang bersamaan
</p>
<a href="@Url.Action("Index")">
    <img alt="Setuju" src="@Url.Content("~/Metro/images/misc/left_round.png")" title="Kembali" />
</a>
<h2>Details</h2>
<table class="bordered hovered">
    <thead>
        <tr class="bg-color-blueLight">
            <th class="text-center">Asal - Tujuan</th>
            <th class="text-center">Kegiatan</th>
            <th class="text-center">Tgl. Pengajuan</th>
            <th colspan="3"></th>
        </tr>
    </thead>
    <tbody>
         <tr class="row bg-color-orangeDark">
            <td>
                @Html.DisplayFor(model => model.FromCountryName) - <br /> @Html.DisplayFor(model => model.ToCountryName)
            </td>
            <td>
                @Html.DisplayFor(model => model.EventName)
            </td>
            <td>
               @Html.DisplayFor(model => model.StartDate) - <br /> @Html.DisplayFor(model => model.EndDate)
            </td>
        </tr>
    </tbody>
</table>
